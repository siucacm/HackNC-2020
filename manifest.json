    "permissions": ["storage", "identity", "contextMenus"],
    "background": {
        "scripts": ["background.js"],
        "persistant": false
    },
    "oauth2": {
        "client_id": "638563984295-bob5nek4mvka3d43iegh50sevn922tk0.apps.googleusercontent.com",
